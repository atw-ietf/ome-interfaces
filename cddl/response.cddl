response = [
  success: { &(entity-id): success-data } / null
  failure: [ + error-data ] / null
  shared: aviation-map
]
success-data = [
  keys: { &(key-id): aviation-map },
  shared: aviation-map
]
error-data = [
  eid: &(entity-id) / null,
  kid: &(key-id) / null,
  cat: any / null,
  msg: any / null
]
response-claim = {
  + &(entity-type): [
    responses: [+ response-datum], 
    chains: {
      canonical_chain: [+ canonical: x509],
      ? be_chain: [+ be: endorsement],
    },
    meta: { metadata } / null
  ]
}
response-datum = [
  unique_id: uint / str / uuid,
  certs: {
    canonical: x509,
    ? be: endorsement,
  },
  meta: { metadata } / null
]